<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <ApplicationIcon>folderimageresizer.ico</ApplicationIcon>
    <Authors>SioN_510N</Authors>
    <Company>siontea</Company>
    <Description>Resize all images from a folder in one go</Description>
    <PackageProjectUrl>https://github.com/SioNisti/FolderImageResizer</PackageProjectUrl>
    <PackageIcon>folderimageresizer.png</PackageIcon>
    <PackageIconUrl />
